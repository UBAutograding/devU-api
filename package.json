{
  "name": "typescript-api-example",
  "version": "1.0.0",
  "description": "Tyepscript Express/ TypeORM Example",
  "scripts": {
    "start": "ts-node-dev src/index.ts",
    "typeorm": "ts-node ./node_modules/typeorm/cli.js  --config src/database.ts",
    "test": "jest --passWithNoTests",
    "clean": "rimraf build/*",
    "lint": "tsc",
    "build": "npm-run-all clean lint",
    "format": "prettier --write \"./**/*.{js,ts,json,md}\"",
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "./**/*.{js,ts,json,md}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16"
  },
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/helmet": "^4.0.0",
    "@types/jest": "^26.0.23",
    "@types/node": "^8.0.29",
    "husky": "^6.0.0",
    "jest": "^27.0.4",
    "lint-staged": "^11.0.0",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.3.0",
    "rmraf": "^1.0.3",
    "ts-jest": "^27.0.2",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "devu-shared-modules": "github:UBAutograding/devu-shared#master",
    "express": "^4.17.1",
    "express-validator": "^6.11.1",
    "helmet": "^4.6.0",
    "morgan": "^1.10.0",
    "pg": "^8.4.0",
    "reflect-metadata": "^0.1.10",
    "swagger-jsdoc": "^6.1.0",
    "swagger-ui-express": "^4.1.6",
    "typeorm": "0.2.32"
  }
}
